<launch>
    <arg name="map_file" default="$$(find qibitech_turtlebot3_world)/maps/map.yaml" />
    <include file="$(find turtlebot3_gazebo)/launch/turtlebot3_world.launch"/>
    <include file="$(find turtlebot3_navigation)/launch/turtlebot3_navigation.launch">
        <arg name="map_file" value="$(arg map_file)"/>
    </include>
    <node name="qibitech_turtlebot3_world" pkg="qibitech_turtlebot3_world" type="move_turtlebot3_world.py"/>
</launch>